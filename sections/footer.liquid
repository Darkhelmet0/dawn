// Function to show modal on homepage with unique storage key
function initializeHomepageWelcomeModal() {
  // Only show on homepage ('/')
  if (window.location.pathname !== '/') return;

  // Check if user has already dismissed the modal
  //const STORAGE_KEY = 'homepage_welcome_modal_dismissed_v1';
  //if (localStorage.getItem(STORAGE_KEY)) return;

  // Add modal to the page
  document.body.insertAdjacentHTML('beforeend', modalHTML);

  const modalOverlay = document.getElementById('homepage-welcome-modal__overlay');
  const closeButton = document.getElementById('homepage-welcome-modal__close-button');

  // Show the modal
  modalOverlay.style.display = 'block';

  // Close modal function
  function closeHomepageWelcomeModal() {
    modalOverlay.style.display = 'none';
    //localStorage.setItem(STORAGE_KEY, 'true');
    
    // Optional: Remove modal from DOM after closing
    setTimeout(() => {
      modalOverlay.remove();
    }, 300);
  }

  // Event listeners for closing
  closeButton.addEventListener('click', closeHomepageWelcomeModal);
  
  // Close on overlay click
  modalOverlay.addEventListener('click', (e) => {
    if (e.target === modalOverlay) {
      closeHomepageWelcomeModal();
    }
  });

  // Close on escape key
  document.addEventListener('keydown', (e) => {
    if (e.key === 'Escape') {
      closeHomepageWelcomeModal();
    }
  });
}

// Initialize when DOM is loaded
document.addEventListener('DOMContentLoaded', initializeHomepageWelcomeModal);
  
</script>
